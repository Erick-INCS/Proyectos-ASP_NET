<!doctype html>
<html class="no-js" lang="">



<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Google Fonts
        ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/bootstrap.min.css">
    <!-- font awesome CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/font-awesome.min.css">
    <!-- owl.carousel CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/owl.carousel.css">
    <link rel="stylesheet" href="~/Content/admin/css/owl.theme.css">
    <link rel="stylesheet" href="~/Content/admin/css/owl.transitions.css">
    <!-- meanmenu CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/meanmenu/meanmenu.min.css">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/animate.css">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/normalize.css">
    <!-- mCustomScrollbar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/scrollbar/jquery.mCustomScrollbar.min.css">
    <!-- jvectormap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/jvectormap/jquery-jvectormap-2.0.3.css">
    <!-- Notika icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/notika-custom-icon.css">
    <!-- wave CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/wave/waves.min.css">
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/main.css">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/style.css">
    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/admin/css/responsive.css">
    <!-- modernizr JS
        ============================================ -->
    <script src="~/Scripts/admin/js/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body>
    <!-- Start Header Top Area -->
    <div class="header-top-area">
        <div class="container">
            <div class="header-top-menu">
                <ul class="nav navbar-nav notika-top-nav">
                    <li class="nav-item dropdown">
                        <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle"><span>Hola @User.Identity.Name</span></a>
                    </li>
                    <li class="nav-item dropdown">
                        @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                        {
                            @Html.AntiForgeryToken()

                            <ul class="nav navbar-nav navbar-right">
                                @*<li>
                    @Html.ActionLink("Hello " + User.Identity.GetUserName() + "!", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage" })
                </li>*@
                                <li>
                                    <a href="javascript:document.getElementById('logoutForm').submit()" class="nav-link dropdown-toggle">
                                        <svg class="bi bi-door-closed" width="1em" height="1em" viewBox="0 0 16 16" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                            <path fill-rule="evenodd" d="M3 2a1 1 0 0 1 1-1h8a1 1 0 0 1 1 1v13a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2zm1 0v13h8V2H4z" />
                                            <path d="M7 9a1 1 0 1 1-2 0 1 1 0 0 1 2 0z" />
                                            <path fill-rule="evenodd" d="M1 15.5a.5.5 0 0 1 .5-.5h13a.5.5 0 0 1 0 1h-13a.5.5 0 0 1-.5-.5z" />
                                        </svg>
                                    </a>
                                </li>
                            </ul>
                        }
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <!-- End Header Top Area -->
    <!-- Main Menu area start-->
    <div class="main-menu-area mg-tb-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <ul class="nav nav-tabs notika-menu-wrap menu-it-icon-pro">
                        <li class="@(ViewBag.Title == "Pedidos" ? "active" : "")">
                            <a data-toggle="tab" href="#Home"><i class="notika-icon notika-house"></i> Pedidos</a>
                        </li>
                        <li class="@(ViewBag.Title == "Productos" ? "active" : "")">
                            <a data-toggle="tab" href="#mailbox"><i class="notika-icon notika-mail"></i> Productos</a>
                        </li>
                        <li class="@(ViewBag.Title == "Categorias" ? "active" : "")">
                            <a data-toggle="tab" href="#Interface"><i class="notika-icon notika-edit"></i> Categorias</a>
                        </li>
                        <li>
                            @*<a data-toggle="tab" href="#Charts"><i class="notika-icon notika-bar-chart"></i> Estatus</a>*@
                        </li>
                    </ul>
                    <div class="tab-content custom-menu-content">
                        <div id="Home" class="tab-pane @(ViewBag.Title == "Pedidos" ? "in active" : "") notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a href="@Url.Action("Index", "Orders")">Ver</a>
                                </li>
                            </ul>
                        </div>
                        <div id="mailbox" class="tab-pane @(ViewBag.Title == "Productos" ? "in active" : "") notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a href="@Url.Action("Index", "Products")">Ver</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Create", "Products")">Agregar</a>
                                </li>
                            </ul>
                        </div>
                        <div id="Interface" class="tab-pane @(ViewBag.Title == "Categorias" ? "in active" : "") notika-tab-menu-bg animated flipInX">
                            <ul class="notika-main-menu-dropdown">
                                <li>
                                    <a href="@Url.Action("Index", "ProductTypes")">Ver</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Create", "ProductTypes")">Agregar</a>
                                </li>
                            </ul>
                        </div>
                        @*<div id="Charts" class="tab-pane notika-tab-menu-bg animated flipInX">
                                <ul class="notika-main-menu-dropdown">
                                    <li>
                                        <a href="index.html">Ver</a>
                                    </li>
                                    <li>
                                        <a href="index-2.html">Agregar</a>
                                    </li>
                                </ul>
                            </div>*@
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Main Menu area End-->
    <!-- Start Status area
    <div class="notika-status-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                    <div class="wb-traffic-inner notika-shadow sm-res-mg-t-30 tb-res-mg-t-30">
                        <div class="website-traffic-ctn">
                            <h2><span class="counter">50,000</span></h2>
                            <p>Pedidos</p>
                        </div>
                        <div class="sparkline-bar-stats1">9,4,8,6,5,6,4,8,3,5,9,5</div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                    <div class="wb-traffic-inner notika-shadow sm-res-mg-t-30 tb-res-mg-t-30">
                        <div class="website-traffic-ctn">
                            <h2>$<span class="counter">90,000</span></h2>
                            <p>Ventas</p>
                        </div>
                        <div class="sparkline-bar-stats2">1,4,8,3,5,6,4,8,3,3,9,5</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
     End Status area-->

    <div class="container">
        @RenderBody()
    </div>

    <div id="fill"></div>

    <!-- Start Footer area-->
    <div class="footer-copyright-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="footer-copy-right">
                        <p>
                            Copyright &copy; 2020 - Perso Coffe
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Footer area-->


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script>
        let fixedSize = window.innerHeight - document.body.offsetHeight// - 91;
        if (fixedSize > 0) document.querySelector("#fill").style.height = fixedSize + "px";
    </script>


    <!-- wow JS
        ============================================ -->
    <script src="~/Scripts/admin/js/wow.min.js"></script>
    <!-- price-slider JS
        ============================================ -->
    <script src="~/Scripts/admin/js/jquery-price-slider.js"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script src="~/Scripts/admin/js/owl.carousel.min.js"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script src="~/Scripts/admin/js/jquery.scrollUp.min.js"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script src="~/Scripts/admin/js/meanmenu/jquery.meanmenu.js"></script>
    <!-- counterup JS
        ============================================ -->
    <script src="~/Scripts/admin/js/counterup/jquery.counterup.min.js"></script>
    <script src="~/Scripts/admin/js/counterup/waypoints.min.js"></script>
    <script src="~/Scripts/admin/js/counterup/counterup-active.js"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="~/Scripts/admin/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <!-- jvectormap JS
        ============================================ -->
    <script src="~/Scripts/admin/js/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="~/Scripts/admin/js/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Scripts/admin/js/jvectormap/jvectormap-active.js"></script>
    <!-- sparkline JS
        ============================================ -->
    <script src="~/Scripts/admin/js/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Scripts/admin/js/sparkline/sparkline-active.js"></script>
    <!-- flot JS
        ============================================ -->
    <script src="~/Scripts/admin/js/flot/jquery.flot.js"></script>
    <script src="~/Scripts/admin/js/flot/jquery.flot.resize.js"></script>
    <script src="~/Scripts/admin/js/flot/curvedLines.js"></script>
    <script src="~/Scripts/admin/js/flot/flot-active.js"></script>
    <!-- knob JS
        ============================================ -->
    <script src="~/Scripts/admin/js/knob/jquery.knob.js"></script>
    <script src="~/Scripts/admin/js/knob/jquery.appear.js"></script>
    <script src="~/Scripts/admin/js/knob/knob-active.js"></script>
    <!--  wave JS
        ============================================ -->
    <script src="~/Scripts/admin/js/wave/waves.min.js"></script>
    <script src="~/Scripts/admin/js/wave/wave-active.js"></script>
    <!--  todo JS
        ============================================ -->
    <script src="~/Scripts/admin/js/todo/jquery.todo.js"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="~/Scripts/admin/js/plugins.js"></script>
    <!-- main JS
        ============================================ -->
    <script src="~/Scripts/admin/js/main.js"></script>
</body>

</html>
